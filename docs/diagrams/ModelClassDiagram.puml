@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyDoctorBase" as ReadOnlyDoctorBase
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class DoctorBase
Class ModelManager
Class UserPrefs

Class UniquePatientList
Class Patient
Class Address
Class Email
Class Name
Class Phone
Class MedicalHistory
Class Nric
Class Gender
Class DateOfBirth
Class Appointment

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

DoctorBase .up.|> ReadOnlyDoctorBase

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyDoctorBase
ModelManager -left-> "1" DoctorBase
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

DoctorBase *--> "1" UniquePatientList
UniquePatientList --> "~* all" Patient
Patient *--> Name
Patient *--> Gender
Patient *--> Nric
Patient *--> Phone
Patient *--> Email
Patient *--> Address
Patient *--> DateOfBirth
Patient *--> MedicalHistory
Patient *--> Appointment

Patient -[hidden]up--> I
UniquePatientList -[hidden]right-> I


Name -[hidden]right-> Nric
Nric -[hidden]right-> Gender
Gender -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email
Email -[hidden]right-> DateOfBirth
DateOfBirth -[hidden]right-> MedicalHistory


ModelManager --> "~* filtered" Patient
@enduml
